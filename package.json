{
  "name": "saas-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:accessibility": "jest tests/accessibility --config=jest.a11y.config.js",
    "test:performance": "lighthouse-ci autorun",
    "test:visual": "percy exec -- cypress run",
    "test:load": "k6 run tests/performance/load-test.js",
    "test:mobile": "cypress run --config=cypress.mobile.config.js",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:accessibility",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:performance",
    "test:local": "node test-critical-paths.js local",
    "test:prod": "node test-critical-paths.js production"
  },
  "dependencies": {
